<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.4/distro-source/core/src/gwt-module.dtd">
<module>

	<!-- inherit as org.xydra.sharedutils.XydraSharedUtils -->

	<!-- Set overrides for non-GWT-able classes to this subdir -->
	<super-source path="gwtemul" />

	<!-- Inherit the core GWT HTTP module. -->
	<inherits name="com.google.gwt.http.HTTP" />

	<source path="">
		<!-- Some of these are overwritten via super-source -->
		<exclude name="ReflectionUtils.java" />
        <exclude name="URLUtils.java" />
        <exclude name="SystemUtils.java" />
        <include name="XyAssert.java"/>
	</source>

</module>