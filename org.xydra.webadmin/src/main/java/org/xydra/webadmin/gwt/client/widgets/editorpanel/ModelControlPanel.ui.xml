<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:ui:com.google.gwt.uibinder http://google-web-toolkit.googlecode.com/svn-history/trunk/user/src/com/google/gwt/uibinder/resources/UiBinder.xsd"
xmlns:ep="urn:import:org.xydra.webadmin.gwt.client.widgets.editorpanel" xmlns:st="urn:import:org.xydra.webadmin.gwt.client.widgets.selectiontree">

	<g:HTMLPanel styleName="modelControlButtonPanel noBorder">

		<g:HorizontalPanel ui:field="mainPanel">

			<g:Button ui:field="expandAllButton">expand all objects</g:Button>
			<g:Button ui:field="loadAllIDsButton">load all IDs from repo</g:Button>
			<g:Button ui:field="loadAllObjectsButton">load all Objects from repo</g:Button>
			<g:Button ui:field="commitModelChangesButton">check changes and maybe commit</g:Button>
			<g:Button ui:field="discardModelChangesButton">discard all changes</g:Button>

		</g:HorizontalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 

