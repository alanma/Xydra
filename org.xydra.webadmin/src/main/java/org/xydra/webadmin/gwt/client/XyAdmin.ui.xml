<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:ui:com.google.gwt.uibinder http://google-web-toolkit.googlecode.com/svn-history/trunk/user/src/com/google/gwt/uibinder/resources/UiBinder.xsd"
	xmlns:xy="urn:import:org.xydra.webadmin.gwt.client">

	<g:HTMLPanel styleName="xyadmin">

		<g:HTML ui:field="title" styleName="title" >Hello World</g:HTML>
		
		<g:TextBox ui:field="name" />
		
		<g:Button ui:field="send" >Send!</g:Button>
		
		<g:HTML ui:field="greeting" />

	</g:HTMLPanel>

</ui:UiBinder> 