<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:ui:com.google.gwt.uibinder http://google-web-toolkit.googlecode.com/svn-history/trunk/user/src/com/google/gwt/uibinder/resources/UiBinder.xsd"
	xmlns:ep="urn:import:org.xydra.webadmin.gwt.client.widgets.editorpanel"
	xmlns:st="urn:import:org.xydra.webadmin.gwt.client.widgets.selectiontree"
	xmlns:d="urn:import:org.xydra.webadmin.gwt.client.widgets.dialogs"
	xmlns:e="urn:import:org.xydra.gwt.editor.value">

	<ui:style>
		@external .addressWidgetStyle;
		.addressWidgetStyle {
		width:
		300px;
		}



	</ui:style>

	<g:HorizontalPanel ui:field="mainPanel">

		<e:XAddressEditor ui:field="addressEditor"  styleName="{style.addressWidgetStyle}"></e:XAddressEditor>
		<g:Button ui:field="loadLocationButton">load Entity</g:Button>
		<g:Button ui:field="addElementButton">add Entity</g:Button>
		<g:Button ui:field="deleteElementButton">delete Entity</g:Button>
		<g:Button ui:field="clearButton">clear Field</g:Button>

	</g:HorizontalPanel>
</ui:UiBinder> 

