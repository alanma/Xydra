<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to="XydraEditor"> <!-- this setting needs to be mirrored in the pom.xml file -->

      <!-- Inherit the core Web Toolkit stuff. -->
      <inherits name='com.google.gwt.user.User'/>
      
      <!-- Inherit the core GWT XML module. -->
      <inherits name='com.google.gwt.xml.XML'/>
      
      <!-- Inherit the Xydra Core -->
      <inherits name='org.xydra.core.XydraCore'/>
      
      <!-- Inherit the GWT-specific Xydra bits -->
      <inherits name='org.xydra.gwt.XydraGwt'/>
      
      <set-property name="user.agent" value="gecko" /> 
      
	    <!-- Inherit Xydra Logging -->
	    <inherits name='org.xydra.log.Xlog'/>
      
	    <!-- add gwt-log support -->
	    <inherits name='com.allen_sauer.gwt.log.gwt-log-OFF' />
       <extend-property name="log_level" values="TRACE"/>
        
      <!--  ENABLED or DISABLED  -->
      <set-property name="log_DivLogger" value="DISABLED" />
      <set-property name="log_FirebugLogger" value="ENABLED" />
      <set-property name="log_GWTLogger" value="ENABLED" />
      
      <source path="" />
      
      <!-- Specify the app entry point class. -->
      <entry-point class='org.xydra.client.gwt.editor.XydraEditor'/>
      
</module>
